    "ts-node": "^8.1.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.13",
    "@types/lodash": "^4.14.136",
    "@types/node": "^10.14.10",
    "jasmine": "^3.4.0",
    "nyc": "^14.1.1",
    "tslint": "^5.16.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0"
